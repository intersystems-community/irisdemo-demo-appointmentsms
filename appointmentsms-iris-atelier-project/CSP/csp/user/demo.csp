<!DOCTYPE html>
<html>
	<head>

		<meta charset="UTF-8">
		<meta http-equiv="cache-control" content="no-cache" />
		<meta http-equiv="expires" content="0" />
		<meta http-equiv="pragma" content="no-cache" />

		<title>IRIS HL7 Appointment Scheduling Demo</title>

		<script language="javascript">

		var ip = window.location.hostname;

		function openWindows()
		{
			var time=1000;
			var callNumber=0
			var timeout = function () { return ++callNumber*time };

			/*
			A timeout between each opened tab is necessary for Safari.
			Otherwise, Safari will simply open all windows on a single tab.
			With the timeout, Safari will first block the popups. Once the user tells
			safari that it should allow popups, the user should be able to try opening all
			windows again and all the tabs will be opened, one every second, on a new tab.
			*/
			
			setTimeout("openCubeAnalyzer()", timeout());
			
			setTimeout("openCubeArchitect()", timeout());
			
			setTimeout("openEMRUI()", timeout());
			
			setTimeout("openMessageTrace()", timeout());
			
			setTimeout("openSQLExplorer()", timeout());
			
			setTimeout("openDTL()", timeout());
			
			setTimeout("openBusinessRule()", timeout());
			
			setTimeout("openProduction()", timeout());
			
			setTimeout("openAWSCredentials()", timeout());
			
			setTimeout("openAWSDeveloperAppsPage()", timeout());
		
		}

		function window_open(url)
		{
			var winReference = window.open();
			winReference.location = url;
			winReference.parent.focus();
		}

		function openProduction()
		{
			window_open('EnsPortal.ProductionConfig.zen?$NAMESPACE=USER&IRISUserName=SuperUser&IRISPassword=SYS');
		}

		/*function openBusinessProcess()
		{
			window_open('EnsPortal.BPLEditor.zen?NAMESPACE=USER&BP=IRISDemo.BP.EvaluateTweet.Process.bpl&IRISUserName=SuperUser&IRISPassword=SYS')
		}*/

		function openMessageTrace()
		{
			window_open('EnsPortal.MessageViewer.zen?$NAMESPACE=USER&IRISUserName=SuperUser&IRISPassword=SYS')
		}

		function openEMRUI()
		{
			window_open('http://' + ip + ':52773/csp/user/emrsimulator.csp?')
		}
		
		function openSQLExplorer()
		{
			window_open('http://' + ip + ':52773/csp/sys/exp/%25CSP.UI.Portal.SQL.Home.zen?$NAMESPACE=USER&$NAMESPACE=USER&IRISUserName=SuperUser&IRISPassword=SYS')
		}

		function openAWSCredentials()
		{
			window_open('EnsPortal.Credentials.zen?$NAMESPACE=USER&$NAMESPACE=APPINT&IRISUserName=SuperUser&IRISPassword=SYS');
		}

		function openAWSDeveloperAppsPage()
		{
			window_open('https://portal.aws.amazon.com/billing/signup#/start');
		}

		function openBusinessRule()
		{
			window_open('http://' + ip + ':52773/csp/user/EnsPortal.RuleEditor.zen?RULE=IRISDemo.EMRHL7FeedRoutingRule&$NAMESPACE=USER&IRISUserName=SuperUser&IRISPassword=SYS');
		}
		
		function openCubeArchitect()
		{
			window_open('http://' + ip + ':52773/csp/user/_DeepSee.UI.Architect.zen?$NAMESPACE=USER&CUBE=Appointment.cube&IRISUserName=SuperUser&IRISPassword=SYS');
		}
		
		function openCubeAnalyzer()
		{
			window_open('http://' + ip + ':52773/csp/user/_DeepSee.UI.Analyzer.zen?$NAMESPACE=USER&CUBE=Appointment.cube&IRISUserName=SuperUser&IRISPassword=SYS');
		}
		
		function openDashboard()
		{
			window_open('http://' + ip + ':52773/csp/user/_DeepSee.UserPortal.DashboardViewer.zen?DASHBOARD=Dashboards/Appointments.dashboard&IRISUserName=SuperUser&IRISPassword=SYS');
		}
		
		function openDTL()
		{
			window_open('http://' + ip + ':52773/csp/user/EnsPortal.DTLEditor.zen?DT=IRISDemo.DTL.HL7ToSMSTransformer.dtl&IRISUserName=SuperUser&IRISPassword=SYS');
		}
		
		function openDemo1()
		{
			window_open('https://youtu.be/04msxC1F-hs');
		}
		
		function openDemo2()
		{
			window_open('https://youtu.be/lp5iNE6WUhc');
		}

		function openInstructions()
		{
			var btn = document.getElementById('instructionsbtn');
			var instructions = document.getElementById('instructions');

			if (btn.value == 'Show Instructions')
			{
				btn.value = 'Hide Instructions';
				instructions.hidden=false;
			}
			else
			{
				btn.value = 'Show Instructions';
				instructions.hidden=true;
			}
		}

		</script>
